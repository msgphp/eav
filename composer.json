{
    "name": "msgphp/eav",
    "type": "library",
    "license": "MIT",
    "description": "MsgPhp Entity-Attribute-Value domain layer",
    "authors": [
        {
            "name": "Roland Franssen",
            "email": "franssen.roland@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "MsgPhp\\Eav\\": ""
        },
        "exclude-from-classmap": ["/Tests/"]
    },
    "require": {
        "msgphp/domain": "^1.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.5",
        "phpunit/phpunit": "^6.4",
        "ramsey/uuid": "^3.7"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
